<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/main.css" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.7.2/css/all.min.css"
    />
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"
    ></script>

    <title>Author Portfolio</title>
  </head>
  <body>
    <div class="frame">
      <div class="main_frame">
        <div class="left_side">
          <div class="left_1">
            <div class="left_1_1">
              <img
                src="/author/images/profile.svg"
                alt="profile image"
                width="156px"
                height="144px"
              />
            </div>
            <div class="left_1_2"><%=user.name%></div>
            <div class="left_1_3"><%=user.profession%></div>
            <div class="left_1_4">
              <a class="link_href" href="">
                <div class="icon">
                  <img src="/author/icons/facebook.svg" class="icon_1" alt="" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="/author/icons/facebook.svg" class="icon_1" alt="" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="/author/icons/facebook.svg" class="icon_1" alt="" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="/author/icons/facebook.svg" class="icon_1" alt="" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="/author/icons/facebook.svg" class="icon_1" alt="" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="/author/icons/facebook.svg" class="icon_1" alt="" />
                </div>
              </a>
            </div>
          </div>
          <div class="seperator"></div>
          <div class="left_2">
            <div class="left_2_1">
              <div class="left_2_1_start">Age:</div>
              <div class="left_2_1_end"><%=user.age%></div>
            </div>
            <div class="left_2_1">
              <div class="left_2_1_start">Residance:</div>
              <div class="left_2_1_end"><%=user.residence%></div>
            </div>
            <div class="left_2_1">
              <div class="left_2_1_start">Freelance:</div>
              <div class="left_2_1_end">Availabe</div>
            </div>
            <div class="left_2_1">
              <div class="left_2_1_start">Adress:</div>
              <div class="left_2_1_end"><%=user.adress%></div>
            </div>
          </div>

          <div class="seperator"></div>
          <div class="left_3">
            <div class="intro_left">Languages</div>
            <div class="left_3_1">
              <div class="left_3_1_start"><%=user.languages[0]%></div>
              <div class="left_3_1_end">100%</div>
            </div>
            <div class="persentages">
              <div class="persentages_in" style="width: 100%"></div>
            </div>
            <div class="left_3_1">
              <div class="left_3_1_start"><%=user.languages[1]%></div>
              <div class="left_3_1_end">80%</div>
            </div>
            <div class="persentages">
              <div class="persentages_in" style="width: 80%"></div>
            </div>
            <div class="left_3_1">
              <div class="left_3_1_start"><%=user.languages[2]%></div>
              <div class="left_3_1_end">60%</div>
            </div>
            <div class="persentages">
              <div class="persentages_in" style="width: 60%"></div>
            </div>
          </div>
          <div class="seperator"></div>
          <div class="left_4">
            <div class="intro_left">Skills</div>
            <div class="left_3_1">
              <div class="left_3_1_start"><%=user.skills[0]%></div>
              <div class="left_3_1_end">90%</div>
            </div>
            <div class="persentages">
              <div class="persentages_in" style="width: 90%"></div>
            </div>
            <div class="left_3_1">
              <div class="left_3_1_start"><%=user.skills[1]%></div>
              <div class="left_3_1_end">85%</div>
            </div>
            <div class="persentages">
              <div class="persentages_in" style="width: 85%"></div>
            </div>
            <div class="left_3_1">
              <div class="left_3_1_start"><%=user.skills[2]%></div>
              <div class="left_3_1_end">80%</div>
            </div>
            <div class="persentages">
              <div class="persentages_in" style="width: 80%"></div>
            </div>
            <div class="left_3_1">
              <div class="left_3_1_start"><%=user.skills[3]%></div>
              <div class="left_3_1_end">75%</div>
            </div>
            <div class="persentages">
              <div class="persentages_in" style="width: 75%"></div>
            </div>
            <div class="left_3_1">
              <div class="left_3_1_start"><%=user.skills[4]%></div>
              <div class="left_3_1_end">85%</div>
            </div>
            <div class="persentages">
              <div class="persentages_in" style="width: 85%"></div>
            </div>
          </div>
          <div class="seperator"></div>
          <div class="left_5">
            <div class="intro_left" style="margin-bottom: 20px">
              Extra Skills
            </div>
            <div class="left_5_1">
              <img
                src="check.svg"
                alt=""
                class="check"
              /><%=user.extra_skills[0]%>
            </div>
            <div class="left_5_1">
              <img
                src="check.svg"
                alt=""
                class="check"
              /><%=user.extra_skills[1]%>
            </div>
            <div class="left_5_1">
              <img
                src="check.svg"
                alt=""
                class="check"
              /><%=user.extra_skills[2]%>
            </div>
            <div class="left_5_1">
              <img
                src="check.svg"
                alt=""
                class="check"
              /><%=user.extra_skills[3]%>
            </div>
          </div>
        </div>
        <div class="midd_side">fds</div>
        <div class="right_side">fds</div>
      </div>
    </div>
  </body>
</html>
